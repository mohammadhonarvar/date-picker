{
  "name": "date-picker-ma",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "scripts": {
    "c": "yarn clean",
    "clean": "del-cli .tsbuildinfo js build",
    "b": "yarn build",
    "build": "npm-run-all --serial clean build:ts build:rollup",
    "build:ts": "tsc --build",
    "build:rollup": "yarn rollup -c",
    "s": "yarn development-serve",
    "d": "npm-run-all --serial build:ts dev-server",
    "built-serve": "es-dev-server --node-resolve --preserve-symlinks --app-index build/index.html  --compatibility none --open",
    "development-serve": "es-dev-server --node-resolve --preserve-symlinks --app-index index.html  --compatibility none --open",
    "w": "yarn watch",
    "watch": "npm-run-all --parallel \"development-serve --watch\" \"build:ts --watch --preserveWatchOutput\"",
    "fu": "yarn full-upgrade"
  },
  "dependencies": {
    "@polymer/polymer": "^3.1.0",
    "@webcomponents/webcomponentsjs": "^2.4.2",
    "lit-element": "^2.4.0",
    "lit-html": "^1.3.0",
    "pwa-helpers": "^0.9.0"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@web/rollup-plugin-copy": "^0.3.0",
    "@web/rollup-plugin-html": "^1.10.0",
    "@web/rollup-plugin-polyfills-loader": "^1.1.0",
    "del-cli": "^3.0.0",
    "es-dev-server": "^1.47.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "polymer-cli": "^1.9.3",
    "polyserve": "^0.27.0",
    "prpl-server": "^1.4.0",
    "rollup": "^2.56.3",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-summary": "^1.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.0.3"
  }
}
