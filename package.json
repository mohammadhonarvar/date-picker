{
  "name": "date-picker-ma",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "scripts": {
    "c": "yarn clean",
    "clean": "del-cli .tsbuildinfo js build dist",
    "b": "yarn build",
    "build": "npm-run-all --serial clean build:ts",
    "build:ts": "tsc --build",
    "build:polymer": "polymer build --auto-base-path",
    "s": "yarn serve",
    "serve": "prpl-server --root dist --host 0.0.0.0 --port 8080",
    "d": "npm-run-all --serial build:ts dev-server",
    "dev-server": "web-dev-server --port 8080 --node-resolve --preserve-symlinks --app-index",
    "w": "yarn watch",
    "watch": "npm-run-all --parallel \"dev-server --watch\" \"build:ts --watch --preserveWatchOutput\""
  },
  "dependencies": {
    "@polymer/polymer": "^3.4.1",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "lit-element": "^3.2.0",
    "lit-html": "^2.2.3",
    "pwa-helpers": "^0.9.1",
    "lit": "^2.2.3"
  },
  "devDependencies": {
    "del-cli": "^4.0.1",
    "@web/dev-server": "^0.1.30",
    "npm-run-all": "^4.1.5",
    "lit-analyzer": "^1.2.1",
    "lit-html": "^2.2.1",
    "polymer-cli": "^1.9.11",
    "ts-lit-plugin": "^1.2.1",
    "typescript": "~4.6.3"
  }
}
