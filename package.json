{
  "name": "date-picker-ma",
  "description": "Jalali support date picker & range picker made by `LIT`(lit-element & lit-html)",
  "repository": "https://github.com/mohammadhonarvar/date-picker",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "scripts": {
    "c": "yarn clean",
    "clean": "del-cli .tsbuildinfo js build",
    "b": "yarn build",
    "build": "npm-run-all --serial clean build:ts build:rollup",
    "build:ts": "tsc --build",
    "build:rollup": "yarn rollup -c",
    "s": "yarn development-serve",
    "d": "npm-run-all --serial build:ts development-serve",
    "built-serve": "es-dev-server --node-resolve --preserve-symlinks --app-index build/index.html  --compatibility none --open",
    "development-serve": "es-dev-server --node-resolve --preserve-symlinks --app-index index.html  --compatibility none --open",
    "w": "yarn watch",
    "watch": "npm-run-all --parallel \"development-serve --watch\" \"build:ts --watch --preserveWatchOutput\"",
    "fu": "yarn full-upgrade"
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "pwa-helpers": "^0.9.1",
    "lit": "^2.2.4"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@web/rollup-plugin-copy": "^0.3.0",
    "@web/rollup-plugin-html": "^1.10.3",
    "@web/rollup-plugin-polyfills-loader": "^1.3.1",
    "del-cli": "^4.0.1",
    "es-dev-server": "^2.1.0",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "npm-run-all": "^4.1.5",
    "polyserve": "^0.27.15",
    "prpl-server": "^1.4.2",
    "rollup": "^2.74.1",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-summary": "^1.4.3",
    "rollup-plugin-terser": "^7.0.2",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-virtual": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@web/dev-server": "^0.1.31",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-wc": "^1.3.2",
    "lerna": "^4.0.0",
    "lit-analyzer": "^1.2.1",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "postcss-selector-parser": "^6.0.10",
    "prettier": "^2.6.2",
    "react-hot-loader": "^4.13.0",
    "ts-lit-plugin": "^1.2.1",
    "typescript": "~4.6.4"
  }
}
